CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/BlurImageProcessor" "${PODS_ROOT}/Headers/CCDataSourceManager" "${PODS_ROOT}/Headers/DCIntrospect-ARC" "${PODS_ROOT}/Headers/EstimoteSDK" "${PODS_ROOT}/Headers/LoremIpsum" "${PODS_ROOT}/Headers/ReactiveCocoa" "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa" "${PODS_ROOT}/Headers/SVProgressHUD" "${PODS_ROOT}/Headers/Tweaks" "${PODS_ROOT}/Headers/pop" "${PODS_ROOT}/Headers/EstimoteSDK"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/EstimoteSDK" "$(PODS_ROOT)/EstimoteSDK/EstimoteSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/BlurImageProcessor" -isystem "${PODS_ROOT}/Headers/CCDataSourceManager" -isystem "${PODS_ROOT}/Headers/DCIntrospect-ARC" -isystem "${PODS_ROOT}/Headers/EstimoteSDK" -isystem "${PODS_ROOT}/Headers/LoremIpsum" -isystem "${PODS_ROOT}/Headers/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/SVProgressHUD" -isystem "${PODS_ROOT}/Headers/Tweaks" -isystem "${PODS_ROOT}/Headers/pop"
OTHER_LDFLAGS = -ObjC -lEstimoteSDK -lc++ -framework Accelerate -framework CoreLocation -framework Foundation -framework MessageUI -framework MobileCoreServices -framework QuartzCore -framework SystemConfiguration -framework UIKit
PODS_ROOT = ${SRCROOT}/Pods